{
    "editor.formatOnSave": true,
    "search.exclude": {
      "**/node_modules": true,
      "**/dist": true
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true,
      "source.fixAll.tslint": true
    },
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  }
  